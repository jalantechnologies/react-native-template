git_url(ENV["IOS_MATCH_REPOSITORY_URL"])

env = ENV["ENV"]

if env == "preview"
  app_identifier(ENV["IOS_APP_IDENTIFIER_PREVIEW"])
  type("appstore")
elsif env == "production"
  app_identifier(ENV["IOS_APP_IDENTIFIER_PRODUCTION"])
  type("appstore")
else
  UI.user_error!("‚ùå Invalid ENV: #{env}")
end
