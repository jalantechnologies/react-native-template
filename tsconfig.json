{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": ["react-native", "jest", "node"],
    "typeRoots": [
      "@types",
      "./node_modules/@types",
      "./src/types"
    ],
    "baseUrl": "..",
    "paths": {
      "@/*": ["./src/*"],
      "types/*": ["./@types/*"]
    }
  }
}
